@model LPM.Web.ViewModels.ProfileViewModel
    <br />
<div class="row">
    <div class="col-md-12">
        <a asp-action="Index" class="float-right" asp-controller="UserContacts">Edit Contacts</a>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        <ul class="list-unstyled team-members">
            @foreach (var item in Model.UserContacts)
            {
                <li>
                    <div class="row">
                        <div class="col-md-3">
                            Contact Name :
                        </div>
                        <div class="col-md-9">
                            @item.ContactName (@item.RelationShip)
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-3">
                            Address :
                        </div>
                        <div class="col-md-9">
                            @item.ContactAddress <br /> @item.ContactNumber
                        </div>
                    </div>
                </li>
            }
        </ul>
    </div>
</div>



